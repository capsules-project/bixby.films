structure (Film) {
  description (A film in the database)
  property (title) {
    type (Title)
    min (Required) max (One)
  }
  property (year) {
    type (Year)
    min (Required) max (One)
  }
  property (id) {
    type (Id)
    min (Required) max (One)
  }
  property (filmInfo) {
    type (FilmInfo)
    min (Optional) max (One)
    lazy-source (GetFilmDetail)
  }
  property (quest) {
    type (Quest)
    min (Optional) max (One)
  }
}
