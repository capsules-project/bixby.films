capsule {
  id (capsules_project.films)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-es-ES)
  }
  capsule-imports {
    import (viv.core) {as (core)}
  }
  store-countries {
    only {
      allow (ES)
    }
  }
  store-sections {
    section (MoviesAndTV)
  }
  runtime-version (2) {
    overrides {
      allow-dialogs-on-detail-pages (true)
    }
  }
}
