layout-macro-def (FilmsList) {
  params {
    param (this) {
      type (Film)
      min (Required) max (One)
    }
  }
  content {
    cell-card {
      slot2 {
        content {
          order (PrimarySecondary)
          primary ("#{value(this.title)}")
          secondary ("#{value(this.year)}")
        }
      }
    }
  }
}